doctype html
link(rel='stylesheet', type='text/css', href='/semantic/dist/semantic.min.css')
script(
  src="https://code.jquery.com/jquery-3.2.1.min.js"
  integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
  crossorigin="anonymous")
script(src='/chart.js/dist/Chart.bundle.min.js')
script(src="https://code.jquery.com/jquery-3.2.1.min.js")
html
  -var menu = {};
  -menu.Home = '/alyumna';
  -menu.Products = '/products';
  -menu.Orders = '/orders';
  -menu.MarginCalculator = '/margin';

  header
    link(rel="icon", href="/images/brand.ico")
    title Alyumna Collection
    .ui.left.fixed.vertical.menu
      .item
        img.ui.image(src='/images/brand.jpg')
      each val, key in menu
        if selected === key
          a.item(href=val, title=key)= key
        else
          a.item(href=val, title=key)= key
  body
    table(width="100%")
      tr
        td(width="210px")
        td
          block content 
    
    
  
      
